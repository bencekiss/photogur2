<h1>Most recent Images</h1>
<%= link_to "New Image", "/images/new" %>

<% if @most_recent_images.empty? %>
  <p>No images to show</p>
<% else %>
  <% @most_recent_images.each do |image| %>
    <p><%= image.artist %>, <%= image.year %></p>
    <p><%= image.title %></p>
    <%= link_to image_tag(image.url), "/images/#{ image.id }" %><br>
    <br>
  <% end %>
<% end %>
